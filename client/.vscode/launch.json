{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Chrome against localhost, with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "file:////E:/react/react-experiments/client/source/es6-amd/index.html",
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Attach to Chrome, with sourcemaps",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		}
	]
}