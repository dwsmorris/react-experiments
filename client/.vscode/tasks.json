{
    "version": "0.1.0",
    "command": "cmd",
    "isShellCommand": true,
    "tasks": [{
        "args": ["/c",
            "${workspaceRoot}/node_modules/.bin/babel", "--presets", "react", "source/jsx", "--out-dir", "source/es6-amd", "&&", 
            "${workspaceRoot}/node_modules/.bin/babel", "--presets", "es2015,react", "source/jsx", "--out-dir", "source/es5-amd", "&&",             
            "${workspaceRoot}/node_modules/.bin/webpack", "source/es5-amd/app.js", "dist/app.js", "&&",
            "${workspaceRoot}/node_modules/.bin/uglifyjs", "dist/app.js", "-o", "dist/app.js", "--compress", "--mangle"
        ],
        "taskName": "babelify",
        "suppressTaskName": true,
        "isBuildCommand": true,
        "isWatching": true
    }]
}